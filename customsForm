<html>
<body>

<ul>
<li id="location0">Name</li>
<li id="location1">Origin</li>
<li id="location2">Flight Number</li>
<li id="location3">Party</li>
</ul>


<script>

var name = "Name: ";
var origin = "Point of Origin: ";
var flight = "Flight Number: ";
var party = "Number of Passengers: ";

document.getElementById("location0").innerHTML = name + prompt("What is the name on your passport?");
document.getElementById("location1").innerHTML = origin + prompt("Where are you from?");
document.getElementById("location2").innerHTML = flight + prompt("What was your flight number?");
document.getElementById("location3").innerHTML = party + prompt("How many passengers are in your party?");

</script>
</body>

</html>
